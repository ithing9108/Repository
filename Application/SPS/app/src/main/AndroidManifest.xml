<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.sps">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".manageLend" />
        <activity android:name=".manageRental" />
        <activity android:name=".detailLend" />
        <activity android:name=".register" />
        <activity android:name=".detailRental" />
        <activity android:name=".category" />
        <activity android:name=".detailSearch" />
        <activity android:name=".SearchList" />
        <activity android:name=".requestProduct" />
        <activity android:name=".AcceptLend" />

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>
