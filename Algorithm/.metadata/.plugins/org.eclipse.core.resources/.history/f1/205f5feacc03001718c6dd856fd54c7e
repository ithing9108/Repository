import java.util.HashSet;
import java.util.Iterator;

class Solution {
    public int solution(int[] A) {
        // write your code in Java SE 8
        HashSet<Integer> set = new HashSet<Integer>();

        for(int i=0;i<A.length;i++){
	        
	        if(set.contains(A[i])){	
		        set.remove(A[i]);
	        }
	        else{
		        set.add(A[i]);
	        }
        }
 
    	return set.iterator().next();
        
    }
}